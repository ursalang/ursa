let a = fn(k: Int, x1: Fn, x2: Fn, x3: Fn, x4: Fn, x5: Fn): Int {
  if k <= 0 {
    x4() + x5()
  } else {
    var m = k
    let b = fn(): Int {
      m := m - 1
      a(m, b, x1, x2, x3, x4)
    }
    b()
  }
}

print(a(10, fn (): Int {1}, fn (): Int {-1}, fn (): Int {-1}, fn (): Int {1}, fn (): Int {0}))
